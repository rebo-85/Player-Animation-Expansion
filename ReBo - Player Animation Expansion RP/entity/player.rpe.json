{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "minecraft:player",
      "materials": {
        "default": "entity_alphatest",
        "cape": "entity_alphatest",
        "animated": "player_animated"
      },
      "textures": {
        "default": "textures/entity/steve",
        "cape": "textures/entity/cape_invisible"
      },
      "geometry": {
        "default": "geometry.humanoid.custom",
        "cape": "geometry.cape"
      },
      "scripts": {
        "scale": "0.9375",
        "initialize": [
          "v.is_holding_right = 0.0;",
          "v.is_blinking = 0.0;",
          "v.last_blink_time = 0.0;",
          "v.hand_bob = 0.0;"
        ],
        "pre_animation": [
          "v.random_side = math.random_integer(1, 2) == 1 ? 'right' : 'left';",
          "v.helmet_layer_visible = 1.0;",
          "v.leg_layer_visible = 1.0;",
          "v.boot_layer_visible = 1.0;",
          "v.chest_layer_visible = 1.0;",
          "v.attack_body_rot_y = Math.sin(360*Math.sqrt(v.attack_time)) * 5.0;",
          "v.tcos0 = (math.cos(query.modified_distance_moved * 38.17) * query.modified_move_speed / v.gliding_speed_value) * 57.3;",
          "v.first_person_rotation_factor = math.sin((1 - v.attack_time) * 180.0);",
          "v.hand_bob = query.life_time < 0.01 ? 0.0 : v.hand_bob + ((query.is_on_ground && query.is_alive ? math.clamp(math.sqrt(math.pow(query.position_delta(0), 2.0) + math.pow(query.position_delta(2), 2.0)), 0.0, 0.1) : 0.0) - v.hand_bob) * 0.02;",

          "v.map_angle = math.clamp(1 - v.player_x_rotation / 45.1, 0.0, 1.0);",
          "v.item_use_normalized = query.main_hand_item_use_duration / query.main_hand_item_max_duration;"
        ],
        "animate": [
          "ac.root"
        ]
      },
      "animations": {
        "a.walk.right": "animation.custom.player.walk.right",
        "a.walk.left": "animation.custom.player.walk.left",
        "a.sprint.right": "animation.custom.player.sprint.right",
        "a.sprint.left": "animation.custom.player.sprint.left",
        "a.punch.right": "animation.custom.player.punch.right",
        "a.punch.left": "animation.custom.player.punch.left",
        "a.sneak": "animation.custom.player.sneak",
        "a.look_at_target": "controller.animation.humanoid.look_at_target",

        "ac.punch": "controller.animation.custom.player.punch",
        "ac.sneak": "controller.animation.custom.player.sneak",
        "ac.move": "controller.animation.custom.player.move",
        "ac.move.walk": "controller.animation.custom.player.move.walk",
        "ac.move.sprint": "controller.animation.custom.player.move.sprint",
        "ac.root": "controller.animation.custom.player.root"


        
